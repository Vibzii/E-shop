files:
  - path: /etc/nginx/conf.d/custom.conf
    mode: "000644"
    owner: root
    group: root
    content: |
      server {
          # ... your Nginx configuration here ...
          types {
              # ... your types definitions here ...
              types_hash_max_size 2048;
              types_hash_bucket_size 128;
          }
      }